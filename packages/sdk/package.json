{
  "name": "clawdnet-sdk",
  "version": "0.1.1",
  "description": "ClawdNet SDK for AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "clawdnet",
    "ai",
    "agents",
    "sdk"
  ],
  "author": "0xSolace",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xSolace/clawdnet"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
